<style>
  .page-hero {
    padding: 3rem 2rem;
    padding-top: 5rem;
    background: transparent;
    color: white;
  }

  .container {
    max-width: 1200px;
    margin: 0 auto;
  }

  .hero-box {
    background: rgba(0, 212, 255, 0.08);
    border: 1px solid rgba(0, 212, 255, 0.3);
    border-radius: 20px;
    padding: 3rem;
    backdrop-filter: blur(10px);
  }

  .breadcrumb {
    margin-bottom: 2rem;
  }

  .breadcrumb a {
    color: rgba(0, 212, 255, 0.9);
    text-decoration: none;
    font-size: 1rem;
    transition: all 0.3s ease;
  }

  .breadcrumb a:hover {
    color: #00D4FF;
    transform: translateX(-5px);
  }

  .project-header {
    display: flex;
    align-items: center;
    gap: 2rem;
  }

  .project-icon {
    font-size: 4rem;
  }

  .hero-box h1 {
    font-size: 3rem;
    margin: 0 0 0.5rem 0;
    background: linear-gradient(135deg, #00D4FF, #0066FF);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
  }

  .hero-subtitle {
    font-size: 1.1rem;
    color: rgba(255, 255, 255, 0.8);
    margin: 0;
  }

  .page-section {
    padding: 3rem 2rem 4rem 2rem;
    background: transparent;
    color: white;
  }

  .section-intro {
    text-align: center;
    margin-bottom: 3rem;
  }

  .section-intro h2 {
    font-size: 2.5rem;
    background: linear-gradient(135deg, #00D4FF, #0066FF);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    margin-bottom: 1rem;
  }

  .section-intro p {
    color: rgba(255, 255, 255, 0.8);
    font-size: 1.1rem;
  }

  .channels-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
    gap: 2rem;
  }

  .channel-card {
    background: rgba(0, 212, 255, 0.05);
    border: 1px solid rgba(0, 212, 255, 0.2);
    border-radius: 20px;
    overflow: hidden;
    transition: all 0.3s ease;
    text-decoration: none;
    color: white;
    display: flex;
    flex-direction: column;
  }

  .channel-card:hover {
    border-color: rgba(0, 212, 255, 0.5);
    transform: translateY(-5px);
    box-shadow: 0 15px 40px rgba(0, 212, 255, 0.3);
  }

  .channel-thumbnail {
    width: 100%;
    height: 200px;
    background: linear-gradient(135deg, rgba(0, 212, 255, 0.2), rgba(0, 102, 255, 0.2));
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
    overflow: hidden;
  }

  .channel-logo {
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 0;
    filter: blur(5px) brightness(0.8) saturate(1.2);
    opacity: 0.5;
    transform: scale(1.1);
  }

  .channel-thumbnail::before {
    content: '';
    position: absolute;
    width: 100%;
    height: 100%;
    background: radial-gradient(circle at center, rgba(0, 212, 255, 0.15), rgba(0, 102, 255, 0.25));
    backdrop-filter: blur(2px);
    z-index: 1;
  }

  .play-icon {
    font-size: 4rem;
    color: rgba(255, 255, 255, 0.9);
    z-index: 2;
    transition: all 0.3s ease;
    text-shadow: 0 2px 10px rgba(0, 0, 0, 0.5);
  }

  .channel-card:hover .play-icon {
    transform: scale(1.2);
    color: #00D4FF;
  }

  .channel-info {
    padding: 2rem;
    flex-grow: 1;
    display: flex;
    flex-direction: column;
  }

  .channel-info h3 {
    color: #00D4FF;
    font-size: 1.5rem;
    margin: 0 0 1rem 0;
  }

  .channel-description {
    color: rgba(255, 255, 255, 0.8);
    line-height: 1.6;
    margin-bottom: 1.5rem;
    flex-grow: 1;
  }

  .channel-stats {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 1rem;
    margin-bottom: 1.5rem;
    padding: 1rem;
    background: rgba(0, 212, 255, 0.05);
    border-radius: 10px;
  }

  .stat {
    text-align: center;
  }

  .stat-value {
    display: block;
    font-size: 1.3rem;
    font-weight: 700;
    color: #00D4FF;
  }

  .stat-label {
    display: block;
    font-size: 0.8rem;
    color: rgba(255, 255, 255, 0.6);
    margin-top: 0.3rem;
  }

  .channel-status {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 1rem;
  }

  .status-label {
    font-size: 0.9rem;
    color: rgba(255, 255, 255, 0.7);
    font-weight: 500;
  }

  .status-badge {
    padding: 0.5rem 1rem;
    border-radius: 15px;
    font-size: 0.9rem;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.5px;
  }

  .status-active {
    background: rgba(0, 255, 100, 0.15);
    border: 1px solid rgba(0, 255, 100, 0.4);
    color: #00FF64;
  }

  .status-inactive {
    background: rgba(255, 0, 0, 0.15);
    border: 1px solid rgba(255, 0, 0, 0.4);
    color: #FF4444;
  }

  .channel-actions {
    display: flex;
    gap: 0.8rem;
    padding-top: 1rem;
    border-top: 1px solid rgba(0, 212, 255, 0.2);
    margin-top: auto;
  }

  .stats-btn {
    flex: 1;
    padding: 0.5rem 1rem;
    background: rgba(0, 212, 255, 0.1);
    color: #00D4FF;
    text-decoration: none;
    border: 1px solid rgba(0, 212, 255, 0.4);
    border-radius: 15px;
    font-weight: 600;
    font-size: 0.9rem;
    transition: all 0.3s ease;
    text-align: center;
  }

  .stats-btn:hover {
    background: rgba(0, 212, 255, 0.2);
    border-color: #00D4FF;
    transform: translateY(-2px);
    box-shadow: 0 5px 15px rgba(0, 212, 255, 0.3);
  }

  .youtube-btn {
    flex: 1;
    padding: 0.5rem 1rem;
    background: linear-gradient(135deg, #FF0000, #DD0000);
    color: white;
    text-decoration: none;
    border-radius: 15px;
    font-weight: 600;
    font-size: 0.9rem;
    transition: all 0.3s ease;
    text-align: center;
    border: none;
  }

  .youtube-btn:hover {
    transform: translateY(-2px);
    box-shadow: 0 5px 15px rgba(255, 0, 0, 0.3);
  }

  .additional-info {
    margin-top: 3rem;
    padding: 2rem;
    background: rgba(0, 212, 255, 0.05);
    border: 1px solid rgba(0, 212, 255, 0.2);
    border-radius: 20px;
    text-align: center;
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 1rem;
  }

  .info-icon {
    font-size: 2.5rem;
